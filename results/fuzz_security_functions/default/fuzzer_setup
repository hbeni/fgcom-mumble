AFL_I_DONT_CARE_ABOUT_MISSING_CRASHES=1
AFL_SKIP_CPUFREQ=1
# command line:
'afl-fuzz' '-i' 'corpus/fuzz_security_functions' '-o' 'results/fuzz_security_functions' '-t' '10000' '--' './test/build-fuzz/fuzz_security_functions' '@@'
