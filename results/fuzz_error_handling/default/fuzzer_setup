AFL_SKIP_CPUFREQ=1
# command line:
'afl-fuzz' '-i' 'corpus/fuzz_error_handling' '-o' 'results/fuzz_error_handling' '-t' '10000' '--' './test/build-fuzz/fuzz_error_handling' '@@'
